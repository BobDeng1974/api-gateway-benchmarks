#!/bin/bash -x

# test02
curl -i -H "x-tyk-authorization: 3f03d372b1ff5d735dfcc2d0a82ad2b3" \
    -H "Content-Type: application/json" \
    -X POST \
    -d '{
            "allowance": 1000,
            "rate": 1000,
            "per": 1,
            "expires": -1,
            "quota_max": -1,
            "quota_renews": 1449051461,
            "quota_remaining": -1,
            "quota_renewal_rate": 60,
            "org_id": "test02",
            "active": true,
            "is_inactive": false,
            "access_rights": {
                "2": {
                    "api_id": "2",
                    "api_name": "test02",
                    "versions": ["Default"]
                }
            },
            "meta_data": { }
        }' \
     "http://localhost:8080/tyk/keys/key02"

